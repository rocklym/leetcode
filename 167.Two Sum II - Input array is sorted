class Solution(object):
    def two_sum(self, numbers, target):
        lpt = 0
        rpt = len(numbers) - 1
        while lpt <= rpt:
            s = numbers[lpt] + numbers[rpt]
            if s == target:
                return [lpt + 1, rpt + 1]
            elif s > target:
                rpt -= 1
            else:
                lpt += 1
